#!/bin/zsh
query=$(cat ~/.config/BraveSoftware/Brave-Browser/Default/Bookmarks | grep "https://*" | sed -e 's/"url": "\(.*\)"/\1/' | awk '{$1=$1};1'| rofi -dmenu -theme "solarized_alternate")

if [ "$query" != "" ]
then
	if [ "$(echo $query | grep 'https://*.*')" != "" ]
	then
		xdg-open "$query"
	else
		xdg-open "https://google.com/search?q=${query}"
	fi
fi
